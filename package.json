{
  "name": "sleek-stream",
  "version": "1.0.0",
  "description": "Hugo Custom Theme using Tailwind Css Scss With Summery Panel Admin Panel and light/dark theme.",
  "scripts": {
    "npm-check": "npm-check -u",
    "prestart": "sass assets/scss/main.scss assets/css/custom.css",
    "start": "hugo server --disableFastRender  --noHTTPCache",
    "dev": "sass assets/scss/main.scss assets/css/custom.css --watch",
    "build": "hugo --minify --cleanDestinationDir"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Armoghan-ul-Mohmin/Sleek-Stream.git"
  },
  "keywords": [
    "Hugo",
    "Theme",
    "TailwindCss",
    "Scss",
    "Custom"
  ],
  "author": "Armoghan-ul-Mohmin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Armoghan-ul-Mohmin/Sleek-Stream/issues"
  },
  "homepage": "https://github.com/Armoghan-ul-Mohmin/Sleek-Stream#readme",
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.x",
    "@tailwindcss/forms": "^0.x",
    "@tailwindcss/typography": "^0.5.x",
    "autoprefixer": "^10.x",
    "postcss": "^8.x",
    "postcss-cli": "^10.x",
    "tailwindcss": "^3.x"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "npm-check": "^6.0.1",
    "sass": "^1.69.4",
    "tailwindcss": "^3.3.3"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ]
}